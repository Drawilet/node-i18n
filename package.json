{
  "name": "@drawilet/i18n",
  "version": "0.1.2",
  "description": "Effortlessly internationalize your app with our npm i18n package. Easily manage translations for diverse audiences, ensuring a user-friendly, global experience.",
  "main": "dist/index.js",
  "bin": {
    "i18n": "cli/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Drawilet/node-i18n.git"
  },
  "author": "Drawilet <drawiletdev@gmail.com>",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": "./dist/index.js",
    "./strategies/*": "./dist/strategies/*.js",
    "./Client": "./dist/Client.js",
    "./Generator": "./dist/Generator.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:strategies": "tsc -p tsconfig.strategies.json",
    "test": "tsx test/index.ts",
    "test:cli": "tsx cli/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^9.2.0",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "http-proxy-agent": "^7.0.0",
    "tsx": "^4.1.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "bugs": {
    "url": "https://github.com/Drawilet/node-i18n/issues"
  },
  "homepage": "https://github.com/Drawilet/node-i18n#readme",
  "directories": {
    "test": "test"
  },
  "keywords": [
    "i18n"
  ]
}
